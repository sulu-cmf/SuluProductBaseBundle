{
    "name": "sulu/product-bundle",
    "type": "sulu-bundle",
    "description": "The Sulu Bundle for managing products",
    "license": "MIT",
    "keywords": ["sulu", "product"],
    "authors": [
        {
            "name": "Sulu Community",
            "homepage": "https://github.com/massiveart/SuluProductBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.4",
        "doctrine/orm": "2.5.*",
        "willdurand/hateoas-bundle": "0.3.*",
        "oro/doctrine-extensions": "1.0.*",
        "symfony-cmf/routing-bundle": "1.2.*",
        "sulu/sulu": "~1.2",
        "sulu/validation-bundle": "~0.1",
        "jackalope/jackalope-doctrine-dbal": "~1.2.0",
        "jackalope/jackalope-jackrabbit": "~1.2.0"
    },
    "require-dev": {
        "zendframework/zendsearch": "@dev",
        "sensio/framework-extra-bundle":  "~3.0",
        "symfony/symfony": "2.8.*",
        "symfony/monolog-bundle": "2.4.*",
        "phpunit/phpunit": "~4.0.0",
        "symfony-cmf/testing": "~1.2",
        "evenement/evenement": "2.0.0 as 1.0.0"
    },
    "autoload": {
        "psr-0": {
            "Sulu\\Bundle\\ProductBundle": ""
        }
    },
    "target-dir": "Sulu/Bundle/ProductBundle"
}
